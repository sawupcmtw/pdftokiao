{
  "name": "pdftokiao",
  "version": "1.0.0",
  "description": "PDF to Kiao Parser - Convert PDF materials to Kiao import format",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "pdftokiao": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pdf",
    "parser",
    "kiao",
    "education",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@ai-sdk/google": "^1.0.0",
    "@trigger.dev/sdk": "^3.0.0",
    "ai": "^4.0.0",
    "commander": "^12.0.0",
    "lru-cache": "^11.0.0",
    "pdf-parse": "^1.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
